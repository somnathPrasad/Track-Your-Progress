  <!-- Custom styles for this template -->
  <link href="css/dashboard.css" rel="stylesheet">
</head>

<body>
  <div class="container">
    <header class="blog-header py-3">
      <div class="row flex-nowrap justify-content-between align-items-center">
        <div class="col-4 pt-1">
          <a class="text-muted" href="#">Subscribe</a>
        </div>
        <div class="col-4 text-center">
          <a class="blog-header-logo text-dark" href="#">DASHBOARD</a>
        </div>
        <div class="col-4 d-flex justify-content-end align-items-center">
          <a class="btn btn-sm btn-outline-secondary" href="/logout">LogOut</a>
        </div>
      </div>
    </header>

    <aside class="main-sidebar">
      <div class="sidebar">
        <div class="sidebar-heading">
          <a href="/addGoal" style="margin:auto; color:white;">Goals</a>
        </div>
        <div>

          <% allGoals.forEach(function(goal){ %>
            <a style="color:white;" class="sidebar-content" href="/dashboard/<%=goal%>"><%= goal %></a>
        <%  }) %>

        </div>
      </div>
    </aside>

    <main>
      <div class=" p-4 p-md-5 text-white rounded">
        <canvas id="canvas" width="1870" height="906" style="z-index: 0; width: 1247px; height: 604px; cursor: move;"></canvas>
      </div>
    </main>
  </div>




    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
    <script src="js/script.js" charset="utf-8"></script>

    <script type="text/javascript">

      var subGoals = <%= subGoals %>;
      var canvas = document.getElementById("canvas");
      var ctx = canvas.getContext("2d");
      var start = 20;
      ctx.lineCap = "round"
      ctx.lineWidth= 10;
      var canvasWidthCenter = $("canvas").innerWidth()/2;
      var canvasHeigthCenter = $("canvas").innerHeight()/2;

      for(var i=0;i<=subGoals;i++){
        ctx.moveTo(start,canvasHeigthCenter);
        ctx.lineTo(start+50,canvasHeigthCenter);
        ctx.stroke();
        ctx.beginPath();
        ctx.arc(start+50,canvasHeigthCenter, 5, 0, 2* Math.PI);
        ctx.fillStyle = "black";
        ctx.fill();
        start = start+50;
      }
      ctx.beginPath();
      ctx.arc(start,canvasHeigthCenter, 15, 0, 2* Math.PI);
      ctx.fillStyle = "black";
      ctx.fill();



    </script>

</body>

</html>
